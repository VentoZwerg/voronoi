  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win portable"
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^36.7.1",
    "electron-builder": "^26.0.12"
